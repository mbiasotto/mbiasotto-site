<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 600 400" style="background: linear-gradient(135deg, #11998e 0%, #38ef7d 100%);">
  <!-- Background -->
  <defs>
    <pattern id="money" width="60" height="60" patternUnits="userSpaceOnUse">
      <circle cx="30" cy="30" r="15" fill="none" stroke="rgba(255,255,255,0.1)" stroke-width="1"/>
      <text x="30" y="35" text-anchor="middle" fill="rgba(255,255,255,0.1)" font-family="Arial" font-size="12">R$</text>
    </pattern>
  </defs>
  <rect width="100%" height="100%" fill="url(#money)"/>
  
  <!-- Calculator -->
  <g transform="translate(200, 100)">
    <rect x="0" y="0" width="120" height="150" rx="10" fill="#2c3e50"/>
    <rect x="10" y="10" width="100" height="30" rx="5" fill="#ecf0f1"/>
    <text x="60" y="30" text-anchor="middle" fill="#2c3e50" font-family="Arial" font-size="16">R$ 5.000</text>
    
    <!-- Buttons -->
    <rect x="15" y="50" width="20" height="15" rx="2" fill="#3498db"/>
    <rect x="40" y="50" width="20" height="15" rx="2" fill="#3498db"/>
    <rect x="65" y="50" width="20" height="15" rx="2" fill="#3498db"/>
    <rect x="90" y="50" width="20" height="15" rx="2" fill="#e74c3c"/>
  </g>
  
  <!-- Money symbol -->
  <g transform="translate(400, 140)">
    <circle cx="0" cy="0" r="40" fill="rgba(255,255,255,0.2)"/>
    <text x="0" y="10" text-anchor="middle" fill="white" font-family="Arial" font-size="36" font-weight="bold">R$</text>
  </g>
  
  <!-- Chart bars -->
  <g transform="translate(100, 250)">
    <rect x="0" y="0" width="20" height="60" fill="rgba(255,255,255,0.8)"/>
    <rect x="30" y="-20" width="20" height="80" fill="rgba(255,255,255,0.8)"/>
    <rect x="60" y="-40" width="20" height="100" fill="rgba(255,255,255,0.8)"/>
  </g>
  
  <!-- Title -->
  <text x="300" y="320" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="24" font-weight="bold">
    Quanto Custa Desenvolver em PHP?
  </text>
  <text x="300" y="350" text-anchor="middle" fill="rgba(255,255,255,0.8)" font-family="Arial, sans-serif" font-size="16">
    Preços Reais e Fatores de Influência
  </text>
</svg> 